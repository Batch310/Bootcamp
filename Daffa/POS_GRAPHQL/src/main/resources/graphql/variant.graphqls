#lihat dari DTO Variant
type Variant {
	id: Long!
	category_id: Long!
	initial: String!
	name: String!
	active:Boolean!
}

extend type Query{
	findAllVariants: [Variant]!
}

extend type Mutation{
	createVariant(
	category_id: Long!,
	initial: String!,
	name: String!,
	active: Boolean!,
	create_by: String): Long
	
	updateVariant(
	id: Long!,
	category_id: Long!
	initial: String!,
	name: String!
	active:Boolean!,
	modify_by: String): Boolean
	
	deleteVariant(
	id:Long!): Boolean
}